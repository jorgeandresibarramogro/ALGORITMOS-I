#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	float a, b, c, p;
	cout<<"Ingrese el primer numero";
	cin>>a;
	cout<<"Ingrese el segundo numero";
	cin>>b;
	cout<<"Ingrese el tercer numero";
	cin>>c;
	p=(a+b+c)/3;
	cout<<"El promedio de los tres numeros es: "<<p;
	getch();
}
